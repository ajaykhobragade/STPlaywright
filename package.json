{
    "devDependencies": {
        "@babel/core": "^7.11.0",
        "@babel/plugin-transform-runtime": "^7.11.0",
        "@babel/preset-env": "^7.11.0",
        "@babel/register": "^7.10.5",
        "@istanbuljs/nyc-config-babel": "^3.0.0",
        "babel": "^6.23.0",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-node": "0.0.1-security",
        "babel-plugin-__coverage__": "^11.0.0",
        "babel-plugin-istanbul": "^6.0.0",
        "babel-preset-env": "^1.7.0",
        "eslint-plugin-jest-playwright": "^0.2.1",
        "expect-playwright": "^0.2.5",
        "jest": "^26.1.0",
        "jest-circus": "^26.1.0",
        "jest-playwright": "1.3.0",
        "jest-playwright-preset": "^0.2.5",
        "karma-coverage-istanbul-reporter": "^3.0.3",
        "nyc": "^15.1.0",
        "playwright": "^1.2.1",
        "playwright-chromium": "^1.0.2",
        "playwright-firefox": "^1.0.2",
        "playwright-webkit": "^1.0.2"
    },
    "jest": {
        "preset": "jest-playwright-preset",
        "verbose": true,
        "coverageThreshold": {
            "global": {
                "branches": 80,
                "functions": 80,
                "lines": 80,
                "statements": -10
            }
        },
        "globals": {
            "__DEV__": true
        },
        "snapshotSerializers": [
            "my-serializer-module"
        ],
        "collectCoverage": true
    },
    "scripts": {
        "test": "jest",
        "coverage": "nyc npm run test"
    },
    "dependencies": {
        "babel-istanbul": "^0.12.2",
        "query-selector-shadow-dom": "^0.5.0",
        "url": "^0.11.0"
    },
    "nyc": {
        "require": [
            "./update-package.cjs",
            "@babel/register"
        ],
        "reporter": [
            "lcov",
            "text",
            "html"
        ],
        "all": true,
        "include": [
            "test/Acquisition/Sprint29/*.js",
            "lib/**",
            "node_modules/@my-org/**"
        ],
        "exclude": [
            "**/*.spec.js"
        ],
        "excludeNodeModules": false,
        "sourceMap": false,
        "instrument": false
    }
}
